    <!-- https://vocabs.acdh.oeaw.ac.at/schema#Main -->

    <owl:Class rdf:about="https://vocabs.acdh.oeaw.ac.at/schema#Main"/>
    



    <!-- https://digital-humanities.at/schema#RepoObject -->

    <owl:Class rdf:about="https://digital-humanities.at/schema#RepoObject">
        <rdfs:comment xml:lang="en">Generic entity representing the items in the repository - Collections and Resources, which are modelled as subclasses of Repo Object.</rdfs:comment>
        <rdfs:comment xml:lang="en">Note that other classes, especially Project, are not considered subclasses of Repo Object.</rdfs:comment>
        <rdfs:label>Repo Object</rdfs:label>
    </owl:Class>
   


    <!-- https://digital-humanities.at/schema#Resource -->

    <owl:Class rdf:about="https://digital-humanities.at/schema#Resource">
        
        <rdfs:comment xml:lang="en">Basic entity in the schema containing actual data / content payload; comparable (and mostly equivalent) to files in a file system.</rdfs:comment>
        <rdfs:label xml:lang="en">Resource</rdfs:label>
        <skos:altLabel xml:lang="en">Resource</skos:altLabel>
        <skos:altLabel xml:lang="de">Ressource</skos:altLabel>
    </owl:Class>


    <!-- https://digital-humanities.at/schema#Agent -->

    <owl:Class rdf:about="https://digital-humanities.at/schema#Agent">
        <rdfs:comment xml:lang="en">Entity able to act, in the context of this schema especially be involved in the making, processing, distributing, publishing etc. of a Resource, Collection, Concept or Project.
Note that in contrast to foaf:Agent acdh:Agent can only be a Person or an Organisation (not a software or physical artifact).
The different roles an Agent can assume with respect to a Project, Collection, Resource or Concept are modeled as separate object properties (sub-properties of acdh:contributedTo).</rdfs:comment>
        <rdfs:label xml:lang="en">Agent</rdfs:label>
        <skos:altLabel xml:lang="en">Agent</skos:altLabel>
        <skos:altLabel xml:lang="de">Akteur</skos:altLabel>
		<owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </owl:Class>
    
    
</rdf:RDF>



