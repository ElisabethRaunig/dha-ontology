    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/contributor">
        <owl:equivalentProperty rdf:resource="https://digital-humanities.at/schema#hasContributor"/>
    </owl:ObjectProperty>
    

    <!-- https://digital-humanities.at/schema#hasContributor -->

    <owl:ObjectProperty rdf:about="https://digital-humanities.at/schema#hasContributor">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://digital-humanities.at/schema#isContributorOf"/>
        <rdfs:domain rdf:resource="https://digital-humanities.at/schema#Main"/>
        <rdfs:range rdf:resource="https://digital-humanities.at/schema#Agent"/>
        <rdfs:comment xml:lang="en">Agent (person, group, organisation) (B) who was actively involved in creating/curating/editing a Resource, a Collection or in a Project (A). Any person or institution making a significant contribution to the development and/or maintenance of the resource, but whose contribution does not “fit” to another already defined role. (cf. DataCite: Other)
This is a generic property. Use one of the more specific sub-properties where possible.</rdfs:comment>
        <rdfs:label xml:lang="en">has contributor</rdfs:label>
        <skos:altLabel xml:lang="en">Contributor(s)</skos:altLabel>
        <skos:altLabel xml:lang="de">Mitarbeiter oder Beitragende(r)</skos:altLabel>
        <acdh:ordering rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">60</acdh:ordering>
    </owl:ObjectProperty>
   
   
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatype Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <owl:DatatypeProperty rdf:about="https://digital-humanities.at/schema#hasIdentifier">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://digital-humanities.at/schema#Main"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">Unique identifier given by ACDH and used in ACDH systems, as well as identifiers with a stable URL or URI assigned by other parties, e. g. a DOI, a geonames ID or an URN.
Input as URI.
Handles belong into acdh:hasPID and all other identifiers without URL or URI, like local assigned ones, project numbers or inventory numbers into acdh:hasNonLinkedIdentifier.</rdfs:comment>
        <rdfs:label xml:lang="en">has identifier</rdfs:label>
        <skos:altLabel xml:lang="en">Identifier(s)</skos:altLabel>
        <skos:altLabel xml:lang="de">Identifikator(en)</skos:altLabel>        
    </owl:DatatypeProperty>
    


